{
  "name": "bootcamp",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "start:prod": "node server.js",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "animejs": "^3.2.1",
    "bcrypt": "^5.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongo": "^0.1.0",
    "mongodb": "^4.1.3",
    "mongoose": "^5.13.13",
    "nodemon": "^2.0.14",
    "react-transition-group": "^1.2.1",
    "rfs": "^9.0.5",
    "stripe": "^8.191.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "concurrently": "^6.0.2"
  }
}
