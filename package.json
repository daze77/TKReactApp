    "express": "^4.17.3",
    "if-env": "^1.0.4",
    "minimist": "^1.2.6",
    "mongo": "^0.1.0",
    "mongodb": "^4.5.0",
    "mongoose": "^6.3.0",
    "nodemailer": "^6.7.3",
    "nodemon": "^2.0.15",
    "nth-check": "^2.0.1",
    "react-transition-group": "^4.4.2",
    "rfs": "^9.0.6",
    "string-width": "^5.1.2",
    "strip-ansi": "^7.0.1",
    "stripe": "^8.218.0",
